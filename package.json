{
  "name": "react-metismenu",
  "version": "0.3.1",
  "description": "A ready-to-use menu component for React",
  "main": "dist/react-metismenu.min.js",
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack/dev.conf.js --progress --colors --hot --inline",

    "build-dist-min-js": "webpack --config webpack/dist-min.conf.js --progress --colors",
    "build-dist-js": "webpack --config webpack/dist.conf.js --progress --colors",
    "build-dist": "rm -rf dist && npm run build-dist-js && npm run build-dist-min-js",

    "eslint-confs": "eslint webpack",
    "eslint-src": "eslint src",
    "eslint-dev": "eslint dev",

    "test": "npm run eslint-confs && npm run eslint-src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpertuna/react-metismenu.git"
  },
  "keywords": [
    "metis",
    "menu",
    "react"
  ],
  "author": "H.Alper Tuna <halpertuna@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alpertuna/react-metismenu/issues"
  },
  "homepage": "https://github.com/alpertuna/react-metismenu#readme",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0.0-rc || ^15.0"
  }
}
