{
  "name": "react-metismenu",
  "version": "0.3.1",
  "description": "A ready-to-use menu component for React",
  "main": "dist/react-metismenu.min.js",
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack/dev.conf.js --progress --colors --hot --inline",
    "build-dist-min-js": "webpack --config webpack/dist-min.conf.js --progress --colors",
    "build-dist-js": "webpack --config webpack/dist.conf.js --progress --colors",
    "build-dist": "rm -rf dist && npm run build-dist-js && npm run build-dist-min-js",
    "eslint-confs": "eslint webpack __tests__ src --ext=jsx --ext=js",
    "eslint-src": "eslint src --ext=jsx --ext=js",
    "eslint-dev": "eslint dev --ext=jsx --ext=js",
    "jest": "jest",
    "test": "npm run eslint-confs && npm run eslint-src && npm run jest"
  },
  "jest": {
    "automock": false,
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpertuna/react-metismenu.git"
  },
  "keywords": [
    "metis",
    "menu",
    "react"
  ],
  "author": "H.Alper Tuna <halpertuna@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alpertuna/react-metismenu/issues"
  },
  "homepage": "https://github.com/alpertuna/react-metismenu#readme",
  "devDependencies": {
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "jest": "^14.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-stateless-wrapper": "^1.0.7",
    "react-test-renderer": "^15.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0.0-rc || ^15.0"
  }
}
